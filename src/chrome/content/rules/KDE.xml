<!--
	Nonfunctional subdomains:

		- developer
		- ev
		- manifesto
		- websvn
		- wiki
		- (www.)	(times out)

-->
<ruleset name="KDE (partial)">

	<target host="*.kde.org" />
	<!--
		* for cross-domain cookies.
						-->
	<target host="*.forum.kde.org" />


	<securecookie host="^.+\.kde\.org$" name=".+" />


	<rule from="^http://(blogs|bugs|cdn|community|dot|forum|identity|mail)\.kde\.org/"
		to="https://$1.kde.org/" />

</ruleset>
